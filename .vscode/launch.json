{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug trading_main",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/build-debug/trading_main",
        "args": ["1", "MANUAL"],
        "cwd": "${workspaceFolder}",
        "MIMode": "gdb",
        "miDebuggerPath": "/usr/bin/gdb",
        "preLaunchTask": "CMake build (Debug)",
        "setupCommands": [
          { "text": "-enable-pretty-printing", "ignoreFailures": true }
        ],
        "stopAtEntry": false,
        "externalConsole": false
      }
    ]
  }
  